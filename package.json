{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
